shiftY = e.pageY - cords.top\r\n    const shiftX = e.pageX - cords.left\r\n    item.addEventListener('drag', moveAt)\r\n    function moveAt(event){\r\n      item.style.top = event.pageY- shiftY+'px'\r\n      item.style.left = event.pageX-shiftX+'px'\r\n      item.style.zIndex +=5\r\n    }\r\n\r\n    function getCords(elem){\r\n      let box = elem.getBoundingClientRect();\r\n  return {\r\n    top: box.top + pageYOffset,\r\n    left: box.left + pageXOffset\r\n  };\r\n    }\r\n    item.addEventListener('dragend', endFunc)\r\n    function endFunc(){\r\n      item.removeEventListener('drag',moveAt)\r\n    }\r\n  },\r\n    check(el){\r\n      let bg = el.target\r\nwindow.addEventListener('mousemove', function(e) {\r\n    let x = e.clientX / window.innerWidth;\r\n    let y = e.clientY / window.innerHeight;  \r\n    bg.style.transform = 'translate(-' + x *100 + 'px, -' + y * 200 + 'px)';\r\n});\r\n    },\r\n  randomPos(index){\r\n    console.log(index)\r\n      const cords = {\r\n        top: 5*index+Math.random()*150 + 'px',\r\n        left: 270*index-Math.random()*150+'px',\r\n        zIndex: index\r\n      }\r\n      console.log(cords)\r\n      return cords\r\n    }\r\n  },\r\n  \r\n}\r\n</script>\r\n\r\n<style>\r\n*{\r\n  overflow: hidden;\r\n}\r\n.zero{\r\n  display: flex;\r\n  position: absolute;\r\n}\r\n.parallax {\r\n    height: 100vh;\r\n    position: relative;\r\n    overflow: hidden;\r\n    z-index: 0;\r\n}\r\n.parallax-bg {\r\n  overflow: hidden;\r\n    position: absolute;\r\n    background-image: url(\"../img/Pict-BG.jpg\");\r\n    background-position: center center;\r\n    background-repeat: no-repeat;\r\n    background-size: cover;\r\n    width: 110%;\r\n    height: 110%;\r\n    transition: all 0.1s ease;  \r\n}\r\n\r\n\r\n</style>"]}]}